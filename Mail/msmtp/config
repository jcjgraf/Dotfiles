defaults
auth on
tls	on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile	/var/log/mail/msmtp.log

# graf.jeanclaude@gmail.com
account graf.jeanclaude@gmail.com
host smtp.gmail.com
port 587
from graf.jeanclaude@gmail.com
user graf.jeanclaude@gmail.com
passwordeval "pass google.com/graf.jeanclaude@gmail.com"

